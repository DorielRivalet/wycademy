{
	"type": "module",
	"name": "frontier-compendium",
	"version": "0.2.0",
	"license": "MIT",
	"description": "A compendium of resources for Monster Hunter Frontier Z (MHF-Z).",
	"keywords": [
		"MHF-Z",
		"MHF",
		"Monster Hunter",
		"Monster Hunter Frontier",
		"Monster Hunter Frontier Z"
	],
	"homepage": "https://frontier-compendium.netlify.app/",
